  "r2:create": "wrangler r2 bucket create fwea-audio-files",
  "secrets:put": "echo 'Run: wrangler secret put STRIPE_SECRET_KEY'",
  "secrets:list": "wrangler secret list",
  "tail": "wrangler tail",
  "kv:create": "wrangler kv:namespace create CACHE",
  "kv:create:preview": "wrangler kv:namespace create CACHE --preview",
  "deploy:staging": "wrangler deploy --env staging",
  "kv:create:profanity": "wrangler kv:namespace create PROFANITY_LISTS",

